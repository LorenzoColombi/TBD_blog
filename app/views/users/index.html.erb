<h1>Users</h1>


<%= render :partial => 'layouts/navbar' %>

<h2>User list</h2>

<ul>
  <% @users.each do |user| %>
    <li>Email:
      <%= link_to user.email, user_path(user) %>
      <p>Name:  <%= user.name %> </p>
      <p>Surname:  <%= user.surname %> </p>
      <p>Date of birth:  <%= user.dob %> </p>
    </li>
  <% end %>
</ul>

<%= link_to "New user", new_user_path%>